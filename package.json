{
  "enabledApiProposals": [
    "terminalDataWriteEvent",
    "terminalDimensions"
  ],
  "name": "codeHistories",
  "displayName": "code-histories",
  "description": "Tracking user's Python code state and output on VSCode",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codeHistories.codeHistories"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeHistories.codeHistories",
        "title": "Code Histories"
      },
      {
        "command": "codeHistories.checkAndCommit",
        "title": "Code Histories commit",
        "icon": "$(run-all)"
      },
      {
        "command": "codeHistories.undoCommit",
        "title": "Undo Code Histories commit",
        "icon": "$(clock)"
      },
      {
        "command": "codeHistories.selectGitRepo",
        "title": "Code Histories: Select git repo"
      },
      {
        "command": "codeHistories.setNewCmd",
        "title": "Code Histories: Set new command"
      },
      {
        "command": "codeHistories.rationaleInfo",
        "title": "Code Histories: Request rationale info"
      }
    ],
    "keybindings": [
      {
        "command": "codeHistories.selectGitRepo",
        "key": "Ctrl+Shift+G",
        "mac": "Cmd+Shift+G"
      },
      {
        "command": "codeHistories.setNewCmd",
        "key": "Ctrl+Shift+C",
        "mac": "Cmd+Shift+C"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "codeHistories.undoCommit",
          "group": "navigation@0"
        },
        {
          "command": "codeHistories.checkAndCommit",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "active-win": "^8.1.0",
    "simple-git": "^3.5.0"
  }
}
